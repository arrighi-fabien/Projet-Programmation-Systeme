<Window x:Class="EasySaveGUI.SaveJobRunWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EasySaveGUI"
        mc:Ignorable="d"
        Title="EasySave Run SaveJob" Height="166" Width="302" Icon="/icon.png" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize">
    <Grid Background="{StaticResource MainBackground}">
        <Grid HorizontalAlignment="Center" VerticalAlignment="Top" Width="122" Margin="0,56,0,0">
            <TextBlock x:Name="textBlock" HorizontalAlignment="Left" TextWrapping="Wrap" Text="Executing :" VerticalAlignment="Center" FontSize="16" Foreground="{StaticResource ForegroundBrush}"/>
            <TextBlock x:Name="runningSaveJobTextBlock" HorizontalAlignment="Right" Margin="62,0,0,0" TextWrapping="Wrap" Text="Save1" VerticalAlignment="Center" FontSize="16" Foreground="{StaticResource ForegroundBrush}"/>
        </Grid>
    </Grid>
</Window>
